class Docente

instance variables
  codigo: token;
  nombres: seq of char := "???";
  apellidos: seq of char := "???";
  inv len nombres >= 3 and len apellidos >= 3;

operations
-- constructor para Docente
public Docente: token * seq of char * seq of char ==> Docente
Docente(cod, nom, ape) == (
  codigo := cod;
  nombres := nom;
  apellidos := ape;
)
pre len nom >= 3 and len ape >= 3;

-- obtener datos
public getDatos: () ==> seq of char
getDatos() == (
  return nombres ^ " " ^ apellidos
);

functions

end Docente
